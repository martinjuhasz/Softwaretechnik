# That CONFIG

# Kommentare mit # am ANFANG der Zeile
# Leerzeilen sind erlaubt
# Im Format > key:value < schreiben
# Für die übersicht, am besten mit . ewinzelne abschnitte antrennen, siehe hierzu den Bereich "debug*" als Beispiel
# Im Value-Bereich sind : erlaubt, da am ersten Vorkommen von : getrennt wird.

# debug
debug:true

# logger
logger.use_colors:false

# Rest
rest.port:8081
rest.ip:0.0.0.0

activemq:true